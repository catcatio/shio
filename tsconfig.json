{
  "compilerOptions": {
    "strictFunctionTypes": true,
    "types": ["jest", "node"],
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "allowJs": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "jsx": "react",
    "target": "es2015",
    "lib": [  "es2015", "dom"]
  },
  "exclude": [ ]
}
