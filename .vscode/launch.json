{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run current file",
      "type": "node",
      "request": "launch",
      "args": ["${workspaceRoot}/${relativeFile}"],
      "runtimeArgs": ["-r", "ts-node/register"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "Start local development",
      "type": "node",
      "request": "launch",
      "args": ["${workspaceRoot}/index.ts"],
      "runtimeArgs": ["-r", "ts-node/register"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "Run fulfillment playground",
      "type": "node",
      "request": "launch",
      "args": ["${workspaceRoot}/packages/fulfillment/cmd/playground.ts"],
      "runtimeArgs": ["-r", "ts-node/register"],
      "cwd": "${workspaceRoot}/packages/fulfillment",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest Unit Tests",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
      "args": [
        "--config",
        "${workspaceRoot}/jest.json"
      ],
      "internalConsoleOptions": "openOnSessionStart"
  }
  ]
}