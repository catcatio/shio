{
  "name": "@shio-bot/fulfillment",
  "version": "1.0.0",
  "main": "app/index.js",
  "repository": "git@github.com:catcatio/shio.git",
  "author": "Rungsikorn Rungsikavarnich <rungsikorn@me.com>",
  "license": "MIT",
  "nodemonConfig": {
    "delay": "1500",
    "verbose": false,
    "debug": false,
    "execMap": {
      "ts": "node -r ts-node/register"
    },
    "ignore": [
      "node_modules",
      "routes.ts",
      "cmd/playground.ts"
    ],
    "watch": [
      "*.ts"
    ],
    "ext": "ts, tsx",
    "inspect": true
  },
  "scripts": {
    "start": "node cmd/entry.js",
    "dev-remote": "SHIO_LOCAL=0 yarn ts-node ./cmd/entry.ts",
    "prepare-pubsub": "yarn ts-node ./cmd/preparepubsub.ts",
    "dev": "sh ../../scripts/clean.bash && nodemon ./cmd/local.ts",
    "dev:debug": "nodemon --inspect=9239 ./cmd/local.ts",
    "build": "yarn tsc",
    "test": "yarn unit && yarn integration",
    "integration": "yarn jest --runInBand --config jest.integration.json",
    "unit": "yarn jest --runInBand --config jest.unit.json",
    "cloudbuild-reloadcache": "gcloud builds submit --config cloudbuild.node_modules.yaml",
    "cloudbuild-build": "gcloud builds submit --config cloudbuild.yaml",
    "clean": "find -E . -regex '.*\\.(js|map|d\\.ts)' -not -path \"**/node_modules/*\" -not -path  \"./.next/*\" -delete",
    "gcp-deploy": "yarn build && gcloud app deploy service.yaml",
    "gcp-logs": "gcloud app logs tail -s fulfillment",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "@google-cloud/datastore": "^3.1.2",
    "@google-cloud/pubsub": "^0.28.1",
    "@shio-bot/foundation": "^1.1.11",
    "dataloader": "^1.4.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "js-yaml": "^3.13.1",
    "nanoid": "^2.0.3",
    "node-fetch": "^2.6.0",
    "ulid": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/joi": "^14.3.3",
    "@types/node-fetch": "^2.3.4",
    "@types/uuid": "^3.4.4",
    "@types/winston": "^2.4.4",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "nanoid": "^2.0.3",
    "nodemon": "^1.19.1",
    "ora": "^3.4.0",
    "prettier": "1.18.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.4.5"
  }
}
