{
  "compilerOptions": {
    "strictFunctionTypes": true,
    "types": ["jest", "node"],
    "experimentalDecorators": true,
    "noUnusedLocals": false,
    "allowJs": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "jsx": "react",
    "target": "es2015",
    "lib": ["es2015", "dom"],
    "plugins": [ ]
  },
  "exclude": [
    "coverage",
    "jest-stare"
  ]
}
