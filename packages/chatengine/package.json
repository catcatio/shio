{
  "name": "@shio-bot/chatengine",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "find -E . -regex '.*\\.(info|js|map|d\\.ts)' -not -path \"**/node_modules/*\" -not -path  \"./.next/*\" -delete && rm -rf dist",
    "test": "tsc && jest --config jest.unit.json",
    "postinstall": "tsc"
  },
  "bin": {
    "dialogflow-agent": "node /dist/cmd/dialogflow/agent.js"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@line/bot-sdk": "^6.7.1",
    "adm-zip": "^0.4.13",
    "dialogflow": "^0.9.1",
    "fs-extra": "^8.0.1",
    "node-fetch": "^2.6.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.32",
    "@types/fs-extra": "^7.0.0",
    "@types/dialogflow": "^0.9.0",
    "@types/jest": "^24.0.13",
    "@types/yargs": "^13.0.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
